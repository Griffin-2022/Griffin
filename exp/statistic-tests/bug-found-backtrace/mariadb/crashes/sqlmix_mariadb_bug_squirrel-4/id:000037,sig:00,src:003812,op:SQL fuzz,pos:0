CREATE TEMPORARY TABLE IF NOT EXISTS v0 ( v1 TEXT ( 84 ) , v2 NUMERIC UNIQUE CHECK ( v2 NOT LIKE v2 IN ( 'x' , 'x' , 'x' ) AND v2 < -1 ) ) ;
 INSERT INTO v0 ( v2 ) VALUES ( 255 ) ;
 UPDATE v0 SET v1 = -128 WHERE v2 = -128 ;
 INSERT INTO v0 ( v2 ) VALUES ( 82 ) , ( 16 ) ;
 SELECT DISTINCT * FROM ( SELECT DISTINCT v2 / -128 , v2 / 64 FROM v0 ) AS v6 JOIN v0 AS v3 , v0 AS v7 , v0 AS v5 JOIN v0 AS v4 , v0 ;
 