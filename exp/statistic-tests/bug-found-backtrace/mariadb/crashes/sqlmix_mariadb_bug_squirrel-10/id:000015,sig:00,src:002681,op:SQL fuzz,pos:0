CREATE TABLE v0 ( v1 BIGINT UNIQUE ) ;
 INSERT INTO v0 ( v1 ) VALUES ( 22 ) ;
 UPDATE v0 SET v1 = -128 WHERE v1 BETWEEN -1 ^ 10 + 61 AND 19 ;
 INSERT INTO v0 SELECT 127 , -1 * v1 , -32768 * v1 , BIT_OR ( 'x' ) FROM VAR_SAMP ( 93 , 17 ) AS v2 ;
 SELECT DENSE_RANK ( v1 ) OVER w , LEAD ( v1 ) OVER w FROM v0 WINDOW v3 AS ( PARTITION BY v1 ORDER BY v1 DESC ) ;
 