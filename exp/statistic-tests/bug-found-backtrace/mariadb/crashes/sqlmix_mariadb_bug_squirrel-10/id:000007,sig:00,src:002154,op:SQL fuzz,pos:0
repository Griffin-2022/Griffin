CREATE TABLE v0 ( v1 BIGINT UNIQUE ) ;
 INSERT INTO v0 ( v1 ) VALUES ( -1 ) ;
 UPDATE v0 SET v1 = 40 WHERE v1 BETWEEN 0 ^ 0 + -1 AND 20 ;
 INSERT INTO v0 ( v1 ) VALUES ( 127 ) , ( -1 ) ;
 SELECT JSON_ARRAYAGG ( v1 ) OVER w , COUNT ( DISTINCT v1 ) OVER w FROM v0 WINDOW v2 AS ( PARTITION BY v1 ORDER BY v1 DESC ) ;
 