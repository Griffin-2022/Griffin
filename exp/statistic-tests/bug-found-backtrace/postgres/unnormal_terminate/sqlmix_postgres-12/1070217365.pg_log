	           CAST(colname AS a) AS current_check,
	           CAST(current_database() AS a) AS currentid,
	           CAST(cstrschema AS a) AS payload,
	           CAST(cstrname AS a) AS current_check_1
	
	    FROM (
	        /* check constraints */
	        SELECT DISTINCT nr.nspname, r.relname, r.relowner, tab_int.attname, nc.nspname, c.conname
	          FROM pg_namespace nr, pg_class r, pg_attribute tab_int, pg_depend d, pg_namespace nc, pg_constraint c
	          WHERE nr.oid = r.relnamespace
	            AND r.oid = tab_int.attrelid
	            AND d.refclassid = 'pg_catalog.pg_class'::regclass
	            AND d.refobjid = r.oid
	            AND d.refobjsubid = tab_int.attnum
	            AND d.classid = 'pg_catalog.pg_constraint'::regclass
	            AND d.objid = c.oid
	            AND c.connamespace = nc.oid
	            AND c.contype = 'c'
	            AND r.relkind IN ('r', 'p')
	            AND NOT tab_int.attisdropped
	
	        UNION ALL
	
	        /* unique/primary key/foreign key constraints */
	        SELECT nr.nspname, r.relname, r.relowner, tab_int.attname, nc.nspname, c.conname
	          FROM pg_namespace nr, pg_class r, pg_attribute tab_int, pg_namespace nc,
	               pg_constraint c
	          WHERE nr.oid = r.relnamespace
	            AND r.oid = tab_int.attrelid
	            AND nc.oid = c.connamespace
	            AND r.oid = CASE c.contype WHEN 'f' THEN c.confrelid ELSE c.conrelid END
	            AND tab_int.attnum = ANY (CASE c.contype WHEN 'f' THEN c.confkey ELSE c.conkey END)
	            AND NOT tab_int.attisdropped
	            AND c.contype IN ('p', 'u', 'f')
	            AND r.relkind IN ('r', 'p')
	
	      ) AS x (tblschema, tblname, tblowner, colname, cstrschema, cstrname)
	
	    WHERE pg_has_role(x.tblowner, 'USAGE');
