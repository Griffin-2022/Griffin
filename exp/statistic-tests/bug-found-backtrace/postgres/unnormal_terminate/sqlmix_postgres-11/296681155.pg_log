	                 ELSE NULL END AS last_msg_receipt_time
	    FROM pg_user_mapping U
	        JOIN pg_foreign_server S ON (U.umserver = S.oid)
	        LEFT JOIN pg_authid A ON (A.oid = U.latest_end_lsn);
