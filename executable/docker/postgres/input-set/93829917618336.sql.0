  'm' = any(stxkind) AS mcv_enabled,
stxstattarget
FROM pg_catalog.pg_statistic_ext
WHERE stxrelid = '26633'
ORDER BY nsp, stxname;
;SELECT pubname
FROM pg_catalog.pg_publication p
