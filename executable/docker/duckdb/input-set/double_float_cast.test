# name: test/sql/cast/double_float_cast.test
# description: Test out of range double -> float casts
# group: [cast]

;
PRAGMA enable_verification

;
select 1e308::float;

;
select '1e308'::float;
;
inf

;
select '1e310'::double;
;
inf

;
select '-1e308'::float;
;
