# from string casts
;
CREATE TABLE strings AS SELECT * FROM (VALUES ('   '), ('blablabla'), ('-1000'), ('-1'), ('-0'), ('0'), ('4294967295'), ('10000000000')) tbl(s)

;
SELECT s::UINTEGER FROM strings

;
SELECT TRY_CAST(s AS UINTEGER) FROM strings
;
NULL
NULL
NULL
NULL
0
0
4294967295
NULL

