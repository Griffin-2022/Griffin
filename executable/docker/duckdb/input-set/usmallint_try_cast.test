0
65535

# from string casts
;
CREATE TABLE strings AS SELECT * FROM (VALUES ('   '), ('blablabla'), ('-1000'), ('-1'), ('-0'), ('0'), ('65535'), ('100000')) tbl(s)

;
SELECT s::USMALLINT FROM strings

;
SELECT TRY_CAST(s AS USMALLINT) FROM strings
;
NULL
NULL
NULL
NULL
0
0
65535
NULL

