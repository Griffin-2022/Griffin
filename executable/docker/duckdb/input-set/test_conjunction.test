PRAGMA enable_verification

# create table
;
CREATE TABLE a (i integer, j integer);

;
INSERT INTO a VALUES (3, 4), (4, 5), (5, 6)

;
SELECT * FROM a WHERE (i > 3 AND j < 5) OR (i > 3 AND j > 5)
;
5	6

