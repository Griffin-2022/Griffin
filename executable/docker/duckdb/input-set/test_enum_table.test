;
CREATE TABLE person_string (
    name text,
    current_mood text
);

;
INSERT INTO person_string VALUES ('Pedro', 'ok'), ('Mark', 'sad');

;
select name,current_mood::mood from person_string where current_mood = 'sad'
;
Mark	sad

