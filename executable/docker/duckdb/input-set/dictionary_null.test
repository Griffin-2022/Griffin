# name: test/sql/storage/compression/dictionary/dictionary_null.test
# description: Test storage with dictionary compression and many null values
# group: [dictionary]

# load the DB from disk
load __TEST_DIR__/test_rle.db

;
PRAGMA force_compression = 'dictionary'

# single NULL value
;
CREATE TABLE nulls(i VARCHAR)

;
INSERT INTO nulls VALUES (NULL)

;
SELECT * FROM nulls
;
NULL

restart

;
SELECT * FROM nulls
;
NULL

;
DROP TABLE nulls

# many null values
;
CREATE TABLE nulls(i INTEGER)

;
INSERT INTO nulls SELECT NULL FROM range(70000)

;
SELECT COUNT(*), COUNT(i::INT), SUM(i::INT) FROM nulls
;
70000	0	NULL

restart

;
SELECT COUNT(*), COUNT(i::INT), SUM(i::INT) FROM nulls
;
70000	0	NULL

# mix with non-null values
;
INSERT INTO nulls VALUES (1), (1), (1), (2), (2), (2)

;
SELECT COUNT(*), COUNT(i::INT), SUM(i::INT) FROM nulls
;
70006	6	9

restart

;
SELECT COUNT(*), COUNT(i::INT), SUM(i::INT) FROM nulls
;
70006	6	9

;
DROP TABLE nulls
